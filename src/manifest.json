{
  "name": "DOM Size Inspector",
  "version": "@VERSION@",
  "description": "Tools to identify DOM bloat - unnecessary DOM nodes, deeply-nested elements, etc.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "devtools_page": "devtools.html",
  "permissions": ["<all_urls>"],
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "64": "img/64.png",
    "128": "img/128.png",
    "256": "img/256.png"
  },
  "manifest_version": 2
}
